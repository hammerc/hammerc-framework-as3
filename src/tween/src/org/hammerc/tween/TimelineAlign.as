package org.hammerc.tween
{
	/**
	 * <code>TimelineAlign</code> 类枚举了 <code>Timeline</code> 类的缓动方法.
	 * @author wizardc
	 */
	public class TimelineAlign
	{
		/**
		 * 添加的所有缓动对象同时播放.
		 */
		public static const CONCURRENT:int = 0;
		
		/**
		 * 添加的所有缓动对象按其序列播放.
		 */
		public static const SEQUENCE:int = 1;
	}
}
